		context = SpringApplication.run(CarlosGarciaGinerApplication.class, args);
		
		
		Empleado e1 = context.getBean("empleado", Empleado.class);
		//Pelicula p1 = context.getBean("pelicula", Pelicula.class);
		
	}

}
